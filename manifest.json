{
	"manifest_version": 3,
	"name": "Notion Notetaker",
	"description": "Take notes idk",
	"version": "1.0.0",
	"background": {
		"service_worker": "background/background.js"
	},
	"options_page": "options.html",
	"action": {
		"default_popup": "popup/popup.html"
	},
	"permissions": ["tabs", "scripting", "storage"],
	"host_permissions": ["*://*/*"]
}
